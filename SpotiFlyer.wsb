<Configuration>
    <VGpu>Enable</VGpu>
    <Networking>Enable</Networking>
    <MappedFolders>
    </MappedFolders>
    <LogonCommand>
    <Command>C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -executionpolicy unrestricted -command "start powershell {-noexit iex ((New-Object System.Net.WebClient).DownloadString('https://chocolatey.org/install.ps1')); $url = 'https://github.com/Shabinder/SpotiFlyer/releases/latest'; $request = [System.Net.WebRequest]::Create($url); $response = $request.GetResponse(); $realTagUrl = $response.ResponseUri.OriginalString; $version = $realTagUrl.split('/')[-1].Trim('v'); $version; $fileName = "SpotiFlyer-$version.msi"; $realDownloadUrl = $realTagUrl.Replace('tag', 'download') + '/' + $fileName; $realDownloadUrl; Invoke-WebRequest -Uri $realDownloadUrl -OutFile $env:TEMP/$fileName; Start-Process msiexec.exe -Wait -ArgumentList '/I $env:TEMP/$fileName /quiet'; choco install -y javaruntime ffmpeg}"</Command>
    </LogonCommand>
</Configuration>
