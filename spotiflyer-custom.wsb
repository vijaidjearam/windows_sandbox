<Configuration>
    <VGpu>Enable</VGpu>
    <Networking>Enable</Networking>
    <MappedFolders>
    </MappedFolders>
    <LogonCommand>
    <Command>C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -executionpolicy unrestricted -command "start powershell {-noexit iex ((New-Object System.Net.WebClient).DownloadString('https://chocolatey.org/install.ps1')); $url = 'https://github.com/Shabinder/SpotiFlyer/releases/latest'; $request = [System.Net.WebRequest]::Create($url); $response = $request.GetResponse(); $realTagUrl = $response.ResponseUri.OriginalString; $version = $realTagUrl.split('/')[-1].Trim('v'); $version; $fileName = 'SpotiFlyer-'+ $version +'.msi'; $realDownloadUrl = $realTagUrl.Replace('tag', 'download') + '/' + $fileName; $realDownloadUrl; $filelocation = $env:TEMP+'\'+$fileName; $filelocation;$ProgressPreference = 'SilentlyContinue'; Invoke-WebRequest -ContentType "application/octet-stream" -Uri $realDownloadUrl -OutFile $filelocation;write-host 'Download completed waiting for 10 seconds';Start-Sleep -Seconds 10;write-host 'Starting install'; $params = '/I',"$filelocation",'/quiet'; Start-Process 'msiexec.exe' -ArgumentList $params -Wait; choco install -y javaruntime ffmpeg ;$credential = Get-Credential ;New-PSDrive -Name "Z" -Root "\\192.168.1.70\Public\spotiflyer" -Persist -PSProvider "FileSystem" -Credential $credential;New-Item -ItemType SymbolicLink -Path 'C:\Users\WDAGUtilityAccount\SpotiFlyer' -Target 'Z:'}</Command>
    </LogonCommand>
